{
  "name": "@stadionhq/dynamic-yield-js",
  "version": "0.0.5-beta.1",
  "author": "StadionHQ",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stadionhq/dynamic-yield-js.git"
  },
  "keywords": [
    "dynamic-yield",
    "dynamic-yield-js",
    "dynamic-yield-api",
    "dynamic-yield-client",
    "dynamic"
  ],
  "description": "Dynamic Yield API client for JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "preinstall": "cd .api/apis/dy-dev && yarn install && cd ../dy-dev-serve && yarn install",
    "generate-api:dy-dev-serve": "npx api install @dy-dev/v2.3.0#gqe12mawego61 --identifier dy-dev-serve --lang ts --yes",
    "generate-api:dy-dev": "npx api install @dy-dev/v2.3.0#gqeomawegkw6 --identifier dy-dev --lang ts --yes",
    "generate-api": "rm -rf .api && npm run generate-api:dy-dev-serve && npm run generate-api:dy-dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "json-schema-to-ts": "^2.8.0-beta.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@api/dy-dev": "file:.api/apis/dy-dev",
    "@api/dy-dev-serve": "file:.api/apis/dy-dev-serve",
    "fetch-mock": "^12.5.2",
    "lodash.merge": "^4.6.2"
  }
}
